# Farcaster Unfollow Script Configuration
# Copy this file to .env and fill in your details.

# Get your API key from Dune Analytics settings: https://dune.com/settings/api-keys
DUNE_API_KEY="YOUR_DUNE_API_KEY"

# Get your API key from the Neynar dashboard: https://neynar.com/
NEYNAR_API_KEY="YOUR_NEYNAR_API_KEY"

# Create and manage signers in the Neynar dashboard.
FARCASTER_SIGNER_UUID="YOUR_FARCASTER_SIGNER_UUID"

# This is your own Farcaster ID.
TARGET_FID="YOUR_FARCASTER_FID"
